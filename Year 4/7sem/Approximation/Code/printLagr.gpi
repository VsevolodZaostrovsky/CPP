#! /usr/bin/gnuplot -persist
set terminal png size 1000,1000 enhanced font "Helvetica,20"
set output "huge_amount_of_knots.png"

set style line 1 lt 1 linecolor rgb "red" lw 1 pt 1
set style line 2 lt 1 linecolor rgb "blue" lw 1 pt 1

set yrange [0:10]
set xrange [1:10000]

set grid

plot "huge_amount_of_knots.txt" using 1:3 ls 1 title "Lagrange Polynom" with lines ,\
  log(x) ls 2 title "Original function"

